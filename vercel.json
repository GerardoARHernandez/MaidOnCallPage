{
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },  
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }